<div class="page-header">
  <h1>
    Users
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        
        <th>User</th>
        <th>Email</th>

      </tr>

      <% @users.each do |user| %>
      <tr>
        <td>
        <h2><a href="/users/<%= user.id %>"><%= user.username %></a></h2>
            <a href="/users/<%= user.id %>"><img src=<%= user.avatar_url %> class="img-circle"></a>
          
        
        </td>
        <td>
          <%= user.email %>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>